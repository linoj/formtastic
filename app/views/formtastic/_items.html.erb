<li id="<%= wrapper[:id] %>" class="<%= wrapper[:class] %>" >
  <fieldset>
    <legend class="label">
      <%= label -%>
    </legend>
    <ol>
      <% items.each do |item| -%>
      <li <%= html_attr wrapper %>>
        <label for="<%= item[:input_id] %>">
          <%= item[:input] -%>
          <%= item[:label] -%>
        </label>
      </li>
      <% end -%>
    </ol>
  </fieldset>
  <%= hint -%>
  <%= inline_errors -%>
</li>
